<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.integrate.prominent"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />
    <uses-permission android:name="android.permission.SET_WALLPAPER"/><uses-permission android:name="android.permission.WAKE_LOCK" /><uses-permission android:name="android.permission.INTERNET" /><uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" /><uses-permission android:name="android.permission.VIBRATE" />
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Black"
        >
        <activity
            android:name="com.integrate.prominent.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.integrate.prominent.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.integrate.prominent.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.AboutMe"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Dialog"
            >
            <intent-filter>
                <action android:name="com.integrate.prominent.ABOUT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.Prefs"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.integrate.prominent.PREFS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.TextPlay"
            android:theme="@android:style/Theme.Translucent"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.Email"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.Camera"
            android:label="Camera Application" 
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="com.integrate.prominent.Data"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.OpenedClass"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.GFX"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.GFXSurface"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.SoundStuff"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.Slider"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.Tabs"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.SimpleBrowser"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.Flipper"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.SharedPrefs"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.InternalData"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.ExternalData"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.SQLiteExample"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.SQLView"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.integrate.prominent.SQLVIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.Accelerate"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.HttpExample"
            android:label="@string/app_name" >
        </activity>
        
        <receiver 
            android:name="com.integrate.prominent.PointlessWidget"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>
            <meta-data 
                android:name="android.appwidget.provider"
                android:resource="@xml/widget_stuff"
                />
            
        </receiver>
        
        <activity
            android:name="com.integrate.prominent.WidgetConfig"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_CONFIGURE" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.integrate.prominent.GLExample"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.GLCubeRenderer"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.Voice"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.StatusBar"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.integrate.prominent.SeekBarVolume"
            android:label="@string/app_name" >
        </activity>
        
    </application>

</manifest>
